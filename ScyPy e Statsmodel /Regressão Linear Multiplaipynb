    "# Adicionar uma constante(intercepto) no modelo\n",
    "X=sm.add_constant(dad['horas_estudadas'])\n",
    "\n",
    "#Ajustar o modelo de regressão linear simples\n",
    "modelo = sm.OLS(dad['notas'],X).fit()\n",
    "\n",
    "#Exibe o resumo do modelo\n",
    "\n",
    "print(modelo.summary())\n"
   ]
